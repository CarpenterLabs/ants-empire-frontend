{
  "name": "ants-empire-frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.13.1"
  },
  "scripts": {
    "build": "tsc && vite build",
    "serve": "vite preview",
    "preview": "set VITE_HOST_ENV=dev && vite preview",
    "start:local:win": "set VITE_HOST_ENV=local && vite",
    "start:local:linux": "VITE_HOST_ENV=local vite",
    "start:prod:win": "set VITE_HOST_ENV=prod && vite",
    "start:prod:linux": "VITE_HOST_ENV=prod vite",
    "start:nas:linux": "VITE_HOST_ENV=nas vite",
    "build:local:win": "set VITE_HOST_ENV=local && tsc && vite build",
    "build:local:linux": "VITE_HOST_ENV=local && tsc && VITE_HOST_ENV=local vite build",
    "build:prod:win": "set VITE_HOST_ENV=prod && tsc && set VITE_HOST_ENV=prod vite build",
    "build:prod:linux": "VITE_HOST_ENV=prod && tsc && VITE_HOST_ENV=prod vite build",
    "build:nas:linux": "VITE_HOST_ENV=nas && tsc && VITE_HOST_ENV=nas vite build"
  },
  "dependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@nivo/pie": "^0.88.0",
    "@rainbow-me/rainbowkit": "^2.0.7",
    "@tanstack/react-query": "^5.36.0",
    "@types/node": "^18.15.12",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "axios": "1.7.7",
    "bootstrap": "^5.1.3",
    "ethers": "^6.3.0",
    "export-from-json": "^1.6.0",
    "luxon": "^3.3.0",
    "rc-slider": "^11.1.8",
    "rc-switch": "^4.1.0",
    "react": "^18.2.0",
    "react-bootstrap-range-slider": "^3.0.8",
    "react-countup": "^6.3.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.43.9",
    "react-intl": "6.4.1",
    "react-number-format": "^4.9.3",
    "react-router-dom": "^6.2.1",
    "react-toastify": "^10.0.4",
    "reactstrap": "^9.1.8",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "rxjs": "7.8.0",
    "socket.io-client": "^4.8.1",
    "swiper": "^8.2.6",
    "uuid": "^11.0.3",
    "viem": "^2.10.5",
    "wagmi": "^2.12.33",
    "web-vitals": "^1.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/luxon": "^3.3.0",
    "@types/react-helmet": "^6.1.5",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.40.0",
    "eslint-config-react-app": "^7.0.1",
    "sass": "1.62.0",
    "typescript": "5.0.4",
    "vite": "^4.4.9",
    "vite-plugin-checker": "^0.6.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
